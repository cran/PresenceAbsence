citEntry(	entry="article",
  		author =	"Elizabeth A. Freeman and Gretchen Moisen",
 		title =	"{PresenceAbsence}: An R Package for Presence Absence Analysis",
  		journal =	"Journal of Statistical Software",
 		volume =	"23",
  		number =	"11",
  		pages =	"1--31",
  		year = 	"2008",
  		URL =  	"http://www.jstatsoft.org/v23/i11",
  		note         = "eafreeman@fs.fed.us",
  		textVersion =  paste(	"Freeman, E. A. and Moisen, G. (2008). ",
					"PresenceAbsence: An R Package for Presence-Absence Model Analysis. ",
					"Journal of Statistical Software, 23(11):1-31. ",
					"http://www.jstatsoft.org/v23/i11",
   					sep=""))
